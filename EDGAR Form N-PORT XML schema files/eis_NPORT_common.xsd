	 <xs:simpleType name="INTEGER_NON_NEGATIVE_5">
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:totalDigits value="5" />
		</xs:restriction>
	</xs:simpleType>
	<!-- allows negative values -->
	<xs:simpleType name="DECIMAL_TYPE_36_12">
              <xs:union memberTypes="BASE_DECIMAL_TYPE_36_12 INTEGER_TYPE_24"/>
       </xs:simpleType>  
	<xs:simpleType name="BASE_DECIMAL_TYPE_36_12">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="(([+|\-])?\d{1,24}(\.\d{1,12})?)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="INTEGER_TYPE_24">
       <xs:restriction base="xs:integer">
              <xs:totalDigits value="24"/>
       </xs:restriction>
     </xs:simpleType>

	<xs:simpleType name="DECIMAL_NON_NEGATIVE_TYPE_36_12">
		<xs:union memberTypes="BASE_DECIMAL_NON_NEGATIVE_TYPE_36_12 NON_NEGATIVE_INTEGER_TYPE_24"/>
	</xs:simpleType>
	<xs:simpleType name="BASE_DECIMAL_NON_NEGATIVE_TYPE_36_12">
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
			<xs:pattern value="(\d{1,24}(\.\d{1,12})?)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NON_NEGATIVE_INTEGER_TYPE_24">
            <xs:restriction base="xs:nonNegativeInteger">
                   <xs:totalDigits value="24"/>
            </xs:restriction>
     </xs:simpleType>

	<xs:simpleType name="DECIMAL2_TYPE_36_12">
		<xs:union memberTypes="BASE_DECIMAL2_TYPE_36_12 NA_TYPE"/>
	</xs:simpleType>
	<xs:simpleType name="BASE_DECIMAL2_TYPE_36_12">
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="-100"/>
			<xs:pattern value="(([+|\-])?\d{1,24}(\.\d{1,12})?)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="STRING_20_TYPE">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="STRING_5K_TYPE">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="5000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DECIMAL_TYPE_NA_36_12">
		<xs:union memberTypes="BASE_DECIMAL_TYPE_36_12 NA_TYPE"/>
	</xs:simpleType>
	<xs:simpleType name="INTEGER_TYPE_NA_2">
		<xs:restriction base="xs:token">
		<xs:pattern value="[0-9]|[1][0-9]|[2][0-9]|[3][0-1]|(N/A)" />	
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="SERIES_ID_TYPE">
		<xs:annotation>
			<xs:documentation>SEC-supplied series identifier.  Field is 10 characters.  The value must start with an "S" followed by nine digits OR it can take N/A when applied to all series</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[S|s][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CLASS_ID_TYPE">
		<xs:annotation>
			<xs:documentation>SEC-supplied class (contract) identifier.  Field is 10 characters.  The value must start with a "C" followed by nine digits.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[C|c][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
